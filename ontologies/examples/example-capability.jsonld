{
  "@context": "https://ns.slashlife.ai/agent/context/agent.jsonld",
  "id": "cap:generateReport",
  "type": "cap:Capability",
  "label": "Generate Report",
  "description": "Generates a summary report from a given dataset.",
  "capabilityExpression": "Generate a summary report from a specified dataset, with an optional output format.",
  "hasSkill": [
    {
      "id": "cap:generateReportSkill",
      "type": "cap:Skill",
      "function": "generateReport",
      "parameters": [
        {
          "name": "dataset_id",
          "datatype": "xsd:string",
          "description": "The identifier for the input dataset.",
          "required": true
        },
        {
          "name": "output_format",
          "datatype": "xsd:string",
          "description": "The desired format for the report (e.g., 'pdf', 'csv').",
          "required": false
        }
      ],
      "returns": [
        {
          "name": "report_artifact_id",
          "datatype": "xsd:string",
          "description": "The identifier for the generated report artifact."
        }
      ]
    }
  ],
  "governance": {
    "requiresDelegation": true,
    "auditLevel": "strict"
  }
}