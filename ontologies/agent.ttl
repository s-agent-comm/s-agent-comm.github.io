@prefix : <https://ns.slashlife.ai/agent/agent#> .
@prefix core: <https://ns.slashlife.ai/agent/core#> .
@prefix del: <https://ns.slashlife.ai/agent/delegation#> .
@prefix cap: <https://ns.slashlife.ai/agent/capability#> .
@prefix econ: <https://ns.slashlife.ai/agent/economic#> .
@prefix contract: <https://ns.slashlife.ai/agent/contract#> .
@prefix life: <https://ns.slashlife.ai/agent/lifecycle#> .
@prefix did: <https://www.w3.org/ns/did#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix id: <https://ns.slashlife.ai/agent/identity#> .

@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

@prefix agent-profile: <https://ns.slashlife.ai/agent/agent-profile#> .

#################################################################
# Ontology Header
#################################################################

<https://ns.slashlife.ai/agent/agent>
    a owl:Ontology ;
    dct:title "Agent Ontology" ;
    dct:description "Defines the structure and properties of an AI Agent, including its identity, purpose, capabilities, and operational aspects." ;
    dct:modified "2025-11-13"^^xsd:date ;
    owl:versionInfo "0.3.0" ;
    owl:imports <https://ns.slashlife.ai/agent/core> ;
    owl:imports <https://ns.slashlife.ai/agent/identity> .

:hasProfile a owl:ObjectProperty ;
    rdfs:label "has profile" ;
    rdfs:domain core:Agent ;
    rdfs:range agent-profile:AgentProfile ;
    rdfs:comment "Links an Agent to its pragmatic / behavioral profile." .

:hasIdentity a owl:ObjectProperty ;
    rdfs:domain core:Agent ;
    rdfs:range id:AgentIdentity ;
    rdfs:label "hasIdentity" .


#################################################################
#   Purpose and Role
#################################################################

:PurposeRole a owl:Class ;
    rdfs:label "PurposeRole" ;
    rdfs:comment "Declares the operational purpose and ownership of the Agent." .

:purposeAndRole a owl:ObjectProperty ;
    rdfs:domain core:Agent ;
    rdfs:range :PurposeRole ;
    rdfs:label "purposeAndRole" .

:objective a owl:DatatypeProperty ;
    rdfs:domain :PurposeRole ;
    rdfs:range xsd:string ;
    rdfs:label "objective" .

:owner a owl:DatatypeProperty ;
    rdfs:domain :PurposeRole ;
    rdfs:range xsd:string ;
    rdfs:label "owner" .





#################################################################
#   Operational Triggers and IO
#################################################################

:OperationalProfile a owl:Class ;
    rdfs:label "OperationalProfile" ;
    rdfs:comment "Triggers, input schema, and output schema of an Agent." .

:operationalTriggersAndIO a owl:ObjectProperty ;
    rdfs:domain core:Agent ;
    rdfs:range :OperationalProfile .

:Trigger a owl:Class ;
    rdfs:label "Trigger" .

:trigger a owl:ObjectProperty ;
    rdfs:domain :OperationalProfile ;
    rdfs:range :Trigger .

:InputSpec a owl:Class ;
    rdfs:label "InputSpec" .

:input a owl:ObjectProperty ;
    rdfs:domain :OperationalProfile ;
    rdfs:range :InputSpec .

:OutputSpec a owl:Class ;
    rdfs:label "OutputSpec" .

:output a owl:ObjectProperty ;
    rdfs:domain :OperationalProfile ;
    rdfs:range :OutputSpec .


#################################################################
#   System Integration / Dependency Graph
#################################################################

:SystemIntegration a owl:Class ;
    rdfs:label "SystemIntegration" ;
    rdfs:comment "Dependency and interface declarations for OS-level agent execution." .

:systemIntegrationAndDependencies a owl:ObjectProperty ;
    rdfs:domain core:Agent ;
    rdfs:range :SystemIntegration .

:dependsOn a owl:ObjectProperty ;
    rdfs:domain :SystemIntegration ;
    rdfs:range core:AgentEntity ;
    rdfs:label "dependsOn" .

:InterfaceSpec a owl:Class ;
    rdfs:label "InterfaceSpec" .

:interface a owl:ObjectProperty ;
    rdfs:domain :SystemIntegration ;
    rdfs:range :InterfaceSpec .


#################################################################
#   Economic Interfaces
#################################################################

:economicInterfaces a owl:ObjectProperty ;
    rdfs:domain core:Agent ;
    rdfs:range econ:WalletSet .

econ:WalletSet a owl:Class ;
    rdfs:label "WalletSet" .

econ:Wallet a owl:Class ;
    rdfs:label "Wallet" .

econ:wallet a owl:ObjectProperty ;
    rdfs:domain econ:WalletSet ;
    rdfs:range econ:Wallet .

econ:address a owl:DatatypeProperty ;
    rdfs:domain econ:Wallet ;
    rdfs:range xsd:string .


#################################################################
#   Identity Interfaces (Multi-DID)
#################################################################

:identityInterfaces a owl:ObjectProperty ;
    rdfs:domain core:Agent ;
    rdfs:range :IdentitySet .

:IdentitySet a owl:Class ;
    rdfs:label "IdentitySet" .

:IdentityBinding a owl:Class ;
    rdfs:label "IdentityBinding" .

:identity a owl:ObjectProperty ;
    rdfs:domain :IdentitySet ;
    rdfs:range :IdentityBinding .

:keyManagementSystem a owl:DatatypeProperty ;
    rdfs:domain :IdentityBinding ;
    rdfs:range xsd:string .


#################################################################
#   Lifecycle Management
#################################################################

:lifecycleManagement a owl:ObjectProperty ;
    rdfs:domain core:Agent ;
    rdfs:range life:LifecycleRecord .

life:LifecycleRecord a owl:Class ;
    rdfs:label "LifecycleRecord" .

life:genesisRecord a owl:ObjectProperty ;
    rdfs:domain life:LifecycleRecord ;
    rdfs:range life:GenesisRecord .

life:GenesisRecord a owl:Class ;
    rdfs:label "GenesisRecord" .

life:state a owl:DatatypeProperty ;
    rdfs:domain life:LifecycleRecord ;
    rdfs:range xsd:string .

life:healthCheck a owl:ObjectProperty ;
    rdfs:domain life:LifecycleRecord ;
    rdfs:range life:HealthCheckSpec .

life:HealthCheckSpec a owl:Class ;
    rdfs:label "HealthCheckSpec" .

life:decommissionRecord a owl:ObjectProperty ;
    rdfs:domain life:LifecycleRecord ;
    rdfs:range life:DecommissionRecord .

life:DecommissionRecord a owl:Class ;
    rdfs:label "DecommissionRecord" .


#################################################################
#   Contractual Interfaces
#################################################################

:contractualInterfaces a owl:ObjectProperty ;
    rdfs:domain core:Agent ;
    rdfs:range contract:ContractSet .

contract:ContractSet a owl:Class ;
    rdfs:label "ContractSet" .

contract:Contract a owl:Class ;
    rdfs:label "Contract" .

contract:contract a owl:ObjectProperty ;
    rdfs:domain contract:ContractSet ;
    rdfs:range contract:Contract .

contract:termsUri a owl:DatatypeProperty ;
    rdfs:domain contract:Contract ;
    rdfs:range xsd:anyURI .


