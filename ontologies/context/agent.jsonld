{
  "@context": {
    "@vocab": "https://ns.slashlife.ai/agent/agent#",
    "core": "https://ns.slashlife.ai/agent/core#",
    "id": "https://ns.slashlife.ai/agent/identity#",
    "cap": "https://ns.slashlife.ai/agent/capability#",
    "del": "https://ns.slashlife.ai/agent/delegation#",
    "life": "https://ns.slashlife.ai/agent/lifecycle#",
    "econ": "https://ns.slashlife.ai/agent/economic#",
    "contract": "https://ns.slashlife.ai/agent/contract#",
    "did_ns": "https://www.w3.org/ns/did#",
    "xsd": "http://www.w3.org/2001/XMLSchema#",

    /* ==== Core Identifiers ==== */

    "id": "@id",
    "type": "@type",

    "Agent": "core:Agent",
    "AgentEntity": "core:AgentEntity",
    "Capability": "core:Capability",
    "Intent": "core:Intent",
    "Delegation": "core:Delegation",
    "Action": "core:Action",
    "ExecutionContext": "core:ExecutionContext",
    "TraceEvent": "core:TraceEvent",
    "Artifact": "core:Artifact",


    /* ==== DID & Identity vocab (W3C-aligned) ==== */

    "did": {
      "@id": "did_ns:did",
      "@type": "@id"
    },
    "publicKey": {
      "@id": "did_ns:publicKey",
      "@type": "xsd:string"
    },
    "controller": {
      "@id": "did_ns:controller",
      "@type": "@id"
    },


    /* ==== Homomorphix OS Agent Model ==== */

    "hasIdentity": "agent:hasIdentity",
    "purpose_and_role": "agent:purposeAndRole",
    "operational_triggers_and_io": "agent:operationalTriggersAndIO",
    "system_integration_and_dependencies": "agent:systemIntegrationAndDependencies",
    "economic_interfaces": "agent:economicInterfaces",
    "identity_interfaces": "agent:identityInterfaces",
    "lifecycle_management": "agent:lifecycleManagement",
    "contractual_interfaces": "agent:contractualInterfaces",


    /* ==== Core Object Properties (from core.ttl) ==== */

    "hasIntent": {
      "@id": "core:hasIntent",
      "@type": "@id"
    },
    "hasCapability": {
      "@id": "core:hasCapability",
      "@type": "@id"
    },
    "executesAction": {
      "@id": "core:executesAction",
      "@type": "@id"
    },
    "producesArtifact": {
      "@id": "core:producesArtifact",
      "@type": "@id"
    },
    "recordedIn": {
      "@id": "core:recordedIn",
      "@type": "@id"
    },
    "contextOf": {
      "@id": "core:contextOf",
      "@type": "@id"
    },


    /* ==== Delegation schema alignment ==== */

    "delegatedBy": {
      "@id": "del:delegatedBy",
      "@type": "@id"
    },
    "delegatesTo": {
      "@id": "del:delegatesTo",
      "@type": "@id"
    },
    "delegationScope": {
      "@id": "del:delegationScope",
      "@type": "@id"
    },


    /* ==== Lifecycle / Genesis / Trust Anchors ==== */

    "genesis_record": "life:genesisRecord",
    "state": "life:state",
    "health_check": "life:healthCheck",
    "decommission_record": "life:decommissionRecord",

    "timestamp": {
      "@id": "core:timestamp",
      "@type": "xsd:dateTime"
    },
    "ledger_event_id": {
      "@id": "life:ledgerEventId",
      "@type": "xsd:string"
    },


    /* ==== Economic Interface (wallet) ==== */

    "wallets": "econ:wallets",
    "address": "econ:walletAddress",
    "currencies": "econ:supportedCurrencies",
    "economicCapabilities": "econ:capabilities",


    /* ==== Contractual Interfaces ==== */

    "contracts": "contract:contracts",
    "terms_uri": {
      "@id": "contract:termsUri",
      "@type": "@id"
    },
    "compensation_terms": "contract:compensationTerms",
    "parties": "contract:parties",
    "scope": "contract:scope",


    /* ==== Utility ==== */

    "description": "core:description"
  }
}
